   "source": [
    "# 7.Program to swap two numbers without using a temp variable"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "95dcbbcf-aedb-42ea-a2bd-ce36bac748f6",
   "metadata": {},
   "outputs": [],
   "source": [
    "a = 100\n",
    "b = 200"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "83891534-b003-4d8b-b405-7e73da4528bd",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Value of a =  100\n",
      "Value of b =  200\n"
     ]
